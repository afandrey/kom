<div class="modal fade" id="bookAt" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content text-center">
            <div class="modal-header">
                <h5 class="modal-title">Select Time</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <label><i class="fas fa-calendar"></i>Date: </label>
                <input id="date" type="date" name="bookDate" class="form-control">
                <label><i class="fas fa-clock"></i>Time: </label>
                <input id="time"type="time" name="bookTime" class="form-control">
            </div>
            <div class="modal-footer">
                <button id="confirmTime" type="button" class="btn btn-dark" data-dismiss="modal">Done</button>
            </div>
        </div>
    </div>
</div>
